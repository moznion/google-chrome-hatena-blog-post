{
	"name":"hatena-blog-post",
	"version":"0.0.2",
  "manifest_version": 2,
	"description":"はてなブログへのちょっとした投稿が出来ます",
  "permissions":[
    "http://blog.hatena.ne.jp/",
    "tabs"
  ],
	"browser_action":{
		"default_icon":"image/icon.png",
		"default_popup":"popup.html"
	},
  "icons":{
    "16":"image/icon16.png",
    "48":"image/icon48.png",
    "128":"image/icon128.png"
  },
  "options_page": "options.html"
}
